<!DOCTYPE html>
<html lang="ru">
    <head>
      
        <script src="~/Scripts/jquery-1.8.2.min.js"></script>
        <link href="~/Content/fotorama.css" rel="stylesheet">
        <script src="~/Scripts/fotorama.js"></script>
        <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/bootstrap/css/bootstrap.css" rel="stylesheet">
        <link href="~/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet" />
        <link href="~/bootstrap/css/bootstrap-responsive.css" rel="stylesheet" />
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
        <meta charset="utf-8" />
        <title>@ViewBag.Title — приложение ASP.NET MVC</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @*@Styles.Render("~/Content/css")*@
        @Styles.Render("~/bootstrap/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
   
    <body>



        
        <div class="container">            
            <div id="login">
                @Html.Partial("_LoginPartial")
            </div>
            <nav class="navbar navbar-default" role="navigation">
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li>@Html.ActionLink("Главная", "Index", "Home")</li>
                        <li>@Html.ActionLink("Услуги","Services","Home")</li>
                        <li>@Html.ActionLink("График приема","Schedule","Home")</li>
                        <li>@Html.ActionLink("Контакт", "Contact", "Home")</li>                               
                        <li>@Html.ActionLink("Персонал", "Index", "Doctor")</li>                                                           
                        <li>@Html.ActionLink("Подразделения", "Index", "Subdivision")</li> 
                        <li>@Html.ActionLink("О нас", "About", "Home")</li>
                        <li>@Html.ActionLink("Новости", "Index", "Article")</li>
                    </ul>                    
                </div>                
            </nav>
        </div>
        <div class="row">
            <div class="col-md-2"></div>
            <div class="col-md-8">
                <div id="body">
                    @RenderSection("featured", required: false)
                    <section class="content-wrapper main-content clear-fix">
                        @RenderBody()
                    </section>
                </div>
        
            </div>   
            <div class="col-md-2"></div>
        </div>

        <footer>
                <div class="well well-sm">
                    <p class="text-muted"> &copy; 2014 -Гродненская городская больница</p>
                </div>
        </footer>
        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)

    </body>
</html>
